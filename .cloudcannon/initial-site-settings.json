{
    "ssg": "hugo",
    "build": {
      "install_command": "npm i",
      "build_command": "npm run build",
      "output_path": "public",
      "preserved_paths": "node_modules/,.hugo_cache/,resources/",
      "hugoVersion": "0.128.1",
      "environment_variables": [
        {
          "key": "HUGO_CACHEDIR",
          "value": "/usr/local/__site/src/.hugo_cache/"
        }
      ]
    }
}